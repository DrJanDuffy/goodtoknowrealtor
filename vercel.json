{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/blog/sync",
      "schedule": "0 */6 * * *"
    }
  ],
  "functions": {
    "src/app/api/**/route.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 60
    }
  }
}
